  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Open Chrome to whyd22",
      "url": "http://localhost:3000/whyd/bebenebenebeb",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "Open Firefix to whyd22",
      "url": "http://localhost:3000/whyd/bebenebenebeb",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
